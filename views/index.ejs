<div class="art-container">
    <% text.forEach(function(data) {%>
      <div id="articles">
        <div class="card">
            <div class="art-img">
                <a href="#"><svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" fill="currentColor" class="bi bi-suit-heart-fill" viewBox="0 0 16 16" id="fav-mark">
                <path d="M4 1c2.21 0 4 1.755 4 3.92C8 2.755 9.79 1 12 1s4 1.755 4 3.92c0 3.263-3.234 4.414-7.608 9.608a.513.513 0 0 1-.784 0C3.234 9.334 0 8.183 0 4.92 0 2.755 1.79 1 4 1z"/></svg>
                </a>
            </div>
            <div class="art-text">
                <h2><%= data.brand%></h2>
                <p><%= data.model%></p>
                <p><%= data.price%> SEK</p>
                <a href="/<%= data.id%>" class="art-link">Mer info</a>
            </div>
        </div>
    </div>
    <%})%>
  </div>

